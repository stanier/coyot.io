include /includes/mixins

doctype html
html(ng-app='coyot.io')
    head
        include /includes/links
        include /includes/scripts
    body
        +navbar
        
        .container(style='padding-top: 20px'): .row
            .col-sm-3.col-md-2.sidebar
                ul.nav.nav-sidebar
                    +panel_server
                    
                    li(role='separator').divider
                    
                    +panel_cluster

            .col-sm-9.col-sm-offset-3.col-md-offset-2.col-md-10
                block container
